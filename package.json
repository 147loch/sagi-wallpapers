{
  "name": "sagi-wallpapers",
  "productName": "Sagittarius Eye Wallpapers",
  "version": "2.0.2",
  "description": "Converts screenshots from Elite Dangerous to snazzy Wallpapers for Sagittarius Eye's Patrons.",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color"
  },
  "keywords": [],
  "author": "Lo√Øc Herman",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "sagi_wallpapers"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "147loch",
        "name": "sagi-wallpapers"
      },
      "windowsStoreConfig": {
        "packageName": "sagiwallpapers",
        "name": "sagiwallpapers"
      }
    }
  },
  "dependencies": {
    "data-uri-to-buffer": "^2.0.0",
    "electron-compile": "^6.4.2",
    "electron-is-dev": "^0.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "enmap": "^2.6.3",
    "enmap-level": "^2.1.0",
    "image-size": "^0.6.3",
    "merge-images": "^1.1.0",
    "mime-types": "^2.1.18",
    "moment": "^2.22.2",
    "path": "^0.12.7",
    "sharp": "^0.20.5"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-builder": "^20.25.0",
    "electron-forge": "^5.2.2",
    "electron-packager": "^12.1.0",
    "electron-prebuilt-compile": "2.0.2",
    "electron-reload": "^1.2.2",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7"
  }
}
